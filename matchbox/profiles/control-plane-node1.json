{
    "id": "control-plane-node1",
    "name": "control-plane-node1",
    "boot": {
        "kernel": "/assets/vmlinuz",
        "initrd": [
            "/assets/initramfs.xz"
        ],
        "args": [
            "initrd=initramfs.xz",
            "init_on_alloc=1",
            "init_on_free=1",
            "slab_nomerge",
            "pti=on",
            "console=tty0",
            "console=ttyS0",
            "printk.devkmsg=on",
            "talos.platform=metal",
            "talos.config=http://10.0.112.2/assets/controlplane.yaml"
        ]
    }
}